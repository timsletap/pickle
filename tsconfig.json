{
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@firebase/auth": [
        "./node_modules/@firebase/auth/dist/auth.esm.js"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [

{
  "name": "reactnativeexpo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "concurrently --kill-others \"npm run backend\" \"expo start --lan\"",
    "dev:lan": "concurrently --kill-others \"npm run backend\" \"expo start --lan\"",
    "dev:web": "concurrently --kill-others \"npm run backend\" \"expo start --web\"",
    "dev:android": "concurrently --kill-others \"npm run backend\" \"expo start --android\"",
    "dev:ios": "concurrently --kill-others \"npm run backend\" \"expo start --ios\"",
    "lan": "expo start --lan",
    "backend": "python backend/startup.py",
    "backend:init": "cd backend && python -c \"import sqlite3; exec(open('app/schema.sql').read())\" && python populate_mockdata.py",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@types/papaparse": "^5.5.2",
    "expo": "~54.0.33",
    "expo-av": "^16.0.8",
    "expo-constants": "~18.0.12",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.7.0",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-async-storage": "^0.0.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-reanimated-carousel": "^4.0.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "@tsconfig/react-native": "^3.0.8",
    "@types/react": "~19.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
